        <member name="T:UndertaleModTool.MessageBoxExtensions">
            <summary>
            Provides <see cref="T:System.Windows.MessageBox"/> extensions for <see cref="T:System.Windows.Window"/>s.
            </summary>
        </member>
        <member name="M:UndertaleModTool.MessageBoxExtensions.ShowMessage(System.Windows.Window,System.String,System.String)">
            <summary>
            Shows an informational <see cref="T:System.Windows.MessageBox"/> with <paramref name="window"/> as the parent.
            </summary>
            <param name="window">The parent from which the <see cref="T:System.Windows.MessageBox"/> will show.</param>
            <param name="messageBoxText">A <see cref="T:System.String"/> that specifies the text to display.</param>
            <param name="title">A <see cref="T:System.String"/> that specifies the title bar caption to display.</param>
            <returns><see cref="F:System.Windows.MessageBoxResult.OK"/> or <see cref="F:System.Windows.MessageBoxResult.None"/> if
            the <see cref="T:System.Windows.MessageBox"/> was cancelled.</returns>
        </member>
        <member name="M:UndertaleModTool.MessageBoxExtensions.ShowQuestion(System.Windows.Window,System.String,System.Windows.MessageBoxImage,System.String)">
            <summary>
            Shows a <see cref="T:System.Windows.MessageBox"/> prompting for a yes/no question with <paramref name="window"/> as the parent.
            </summary>
            <param name="window">The parent from which the <see cref="T:System.Windows.MessageBox"/> will show.</param>
            <param name="messageBoxText">A <see cref="T:System.String"/> that specifies the text to display.</param>
            <param name="icon">The <see cref="T:System.Windows.MessageBoxImage"/> to display.</param>
            <param name="title">A <see cref="T:System.String"/> that specifies the title bar caption to display.</param>
            <returns><see cref="F:System.Windows.MessageBoxResult.Yes"/> or <see cref="F:System.Windows.MessageBoxResult.No"/> depending on the users' answer.
            <see cref="F:System.Windows.MessageBoxResult.None"/> if the <see cref="T:System.Windows.MessageBox"/> was cancelled.</returns>
        </member>
        <member name="M:UndertaleModTool.MessageBoxExtensions.ShowWarning(System.Windows.Window,System.String,System.String)">
            <summary>
            Shows a warning <see cref="T:System.Windows.MessageBox"/> with <paramref name="window"/> as the parent.
            </summary>
